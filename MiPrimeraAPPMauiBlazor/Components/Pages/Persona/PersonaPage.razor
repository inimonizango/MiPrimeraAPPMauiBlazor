@page "/persona"
@inject NavigationManager navigationManager

<h3>PersonaPage</h3>
<div class="text-center">
    <button class="btn btn-success" @onclick="nuevaPersona">Nueva Persona</button>
</div>

@code {
    private async Task nuevaPersona()
    {
        navigationManager.NavigateTo("/persona/nuevo");
    }
}
